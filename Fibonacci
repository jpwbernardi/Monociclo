		.data

		.text
		
main:
	add $s0, $0, $v0 	//$s0 -> Valor maximo
	000000 00000 00010 10000 00000 100000
           $0    $v0   $s0
	
inicio:
	add $t0, $0, $0	 	//atual = 0
	000000 00000 00000 01000 00000 100000
	       $0    $0    $t0

	addi $t1, $0, 1		//prox = 1
	001000 00000 01001 00000 00000 000001
	       $0    $t1   offset

	add $s1, $0, $0		//resp = 0
	000000 00000 00000 10001 00000 100000
	       $0    $0    $s1

loop:
	add $s1, $t0, $0	//resp = atual
	000000 01000 00000 10001 00000 100000
	       $t0   $0    $s1 
	
	add $t0, $t1, $0	//atual = prox
	000000 01001 00000 01000 00000 100000
	       $t1    $0    $t0
	       
	add $t1, $s1, $t0	//prox = resp + atual
	000000 10001 01000 01001 00000 100000
	       $s1    $t0    $t1
	
	
	slt $t7, $t1, $s0	//s1 < s0 ? t7 = 1 : t7 = 0
	000000 01001 10000 10111 00000 101010
		   $s1   $s0   $t7
	
	
	beq $t7, $0, fim
	000100 00000 10111 0000000000000001
	
	j loop
	000010 00000000000000000000000100
	
fim:
	nop
